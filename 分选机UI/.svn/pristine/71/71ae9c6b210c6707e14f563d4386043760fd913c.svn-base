AAA,EAAAA,KAAaK,EAAAA,EAAAA,IAAO9F,GAAQ,CAC1BxG,MAAO,KACPb,MAAMmN,EAAAA,EAAAA,IAAO3D,GAAM,GAAGxJ,KACtBK,UAAU8M,EAAAA,EAAAA,IAAO3D,GAAM,GAAGlB,WACzB,KAAM,EAAG,CAAC,OAAQ,gBAEvByB,EAAAA,EAAAA,GAAoB,MAAOmC,GAAa,EACtCY,EAAAA,EAAAA,IAAaE,EAAO,CAAEnM,MAAO,QAC7BiM,EAAAA,EAAAA,KAAaK,EAAAA,EAAAA,IAAO9F,GAAQ,CAC1BxG,MAAO,KACPb,MAAMmN,EAAAA,EAAAA,IAAO3D,GAAM,GAAGxJ,KACtBK,UAAU8M,EAAAA,EAAAA,IAAO3D,GAAM,GAAGlB,WACzB,KAAM,EAAG,CAAC,OAAQ,yBAMlC,SCvRL,MAAM,IAA2B,OAAgB,GAAQ,CAAC,CAAC,YAAY,qBAEvE,W","sources":["webpack://demo/./src/utils/useDraw.ts","webpack://demo/./src/views/center/chart/draw.tsx","webpack://demo/./src/views/lineChart/index.vue?310c","webpack://demo/./src/views/lineChart/chart/draw.tsx","webpack://demo/./src/views/lineChart/index.vue?c7ab","webpack://demo/./src/views/lineChart/index.vue","webpack://demo/./src/views/barChart/index.vue","webpack://demo/./src/views/barChart/chart/draw.tsx","webpack://demo/./src/views/barChart/index.vue?2d09","webpack://demo/./src/components/title.vue?dc4b","webpack://demo/./src/components/title.vue","webpack://demo/./src/views/index/index.vue?7480","webpack://demo/./src/views/index/index.vue"],"sourcesContent":["import { ref } from \"vue\";\n\nexport default function useDraw() {\n  // * 指向最外层容器\n  const appRef = ref();\n\n  // * 定时函数\n  const timer = ref(0);\n  // * 默认缩放值\n  const scale = {\n    width: \"1\",\n    height: \"1\",\n  };\n  // * 设计稿尺寸（px）\n  const baseWidth = 1920;\n  const baseHeight = 1080;\n\n  // * 需保持的比例（默认1.77778）\n  const baseProportion = parseFloat((baseWidth / baseHeight).toFixed(5));\n  const calcRate = () => {\n    // 当前宽高比\n    const currentRate = parseFloat(\n      (window.innerWidth / window.innerHeight).toFixed(5)\n    );\n    if (appRef.value) {\n      if (currentRate > baseProportion) {\n        // 表示更宽\n        scale.width = (\n          (window.innerHeight * baseProportion) /\n          baseWidth\n        ).toFixed(5);\n        scale.height = (window.innerHeight / baseHeight).toFixed(5);\n        appRe